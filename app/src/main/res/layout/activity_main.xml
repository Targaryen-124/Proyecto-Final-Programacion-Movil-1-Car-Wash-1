<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/circuloButton"
        android:layout_width="45dp"
        android:layout_height="44dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/ic_circulo" />

    <TextView
        android:id="@+id/initialLetterTextView"
        android:layout_width="28dp"
        android:layout_height="27dp"
        android:layout_gravity="center"
        android:gravity="center"
        android:textColor="@android:color/black"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/circuloButton"
        app:layout_constraintEnd_toEndOf="@+id/circuloButton"
        app:layout_constraintStart_toStartOf="@+id/circuloButton"
        app:layout_constraintTop_toTopOf="@+id/circuloButton"
        tools:text="J" />

    <Button
        android:id="@+id/btnsCot"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/solicitarcotizacion"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="306dp" />

</androidx.constraintlayout.widget.ConstraintLayout>